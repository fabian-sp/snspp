{
  "instance": {
  	"loss": "logistic",
    "dataset": "higgs",
    "l1": 0.005
  },
  "methods": {
    "snspp": {
      "solver": "snspp",
      "alpha_min": -1,
      "alpha_max": 3,
      "n_alpha": 15,
      "batch": [1e-5, 5e-5],
      "params": {
        "max_iter": 600,
        "sample_style": "constant",
        "reduce_variance": true,
        "measure_freq": 1
      }
    },
    "svrg": {
      "solver": "svrg",
      "alpha_min": -4,
      "alpha_max": 1,
      "n_alpha": 15,
      "batch": [1e-5, 5e-5],
      "params": {
        "n_epochs": 10,
        "measure_freq": 400
      }
    },
    "saga": {
      "solver": "saga",
      "alpha_min": -5,
      "alpha_max": 0,
      "n_alpha": 15,
      "batch": [],
      "params": {
        "n_epochs": 5,
        "measure_freq": 400
      }
    }
  },
  "psi_tol": 0.0001,
  "n_rep": 5,
  "start": 0
}

{
  "instance": {
  	"loss": "logistic",
    "dataset": "madelon",
    "poly": 2,
    "l1": 0.02
  },
  "methods": {
    "snspp": {
      "solver": "snspp",
      "alpha_min": -3,
      "alpha_max": 0,
      "n_alpha": 15,
      "batch": [0.01, 0.05],
      "params": {
        "max_iter": 500,
        "sample_style": "constant",
        "reduce_variance": true,
        "measure_freq": 1
      }
    },
    "svrg": {
      "solver": "svrg",
      "alpha_min": -4,
      "alpha_max": -1,
      "n_alpha": 15,
      "batch": [0.01, 0.05],
      "params": {
        "n_epochs": 30,
        "measure_freq": 10
      }
    },
    "saga": {
      "solver": "saga",
      "alpha_min": -5,
      "alpha_max": -2,
      "n_alpha": 15,
      "batch": [],
      "params": {
        "n_epochs": 30,
        "measure_freq": 10
      }
    }
  },
  "psi_tol": 0.0001,
  "n_rep": 2,
  "start": 0
}

{
  "instance": {
  	"loss": "logistic",
    "dataset": "sido",
    "l1": 0.01
  },
  "methods": {
    "snspp": {
      "solver": "snspp",
      "alpha_min": 0,
      "alpha_max": 3,
      "n_alpha": 15,
      "batch": [0.001,0.005,0.01],
      "params": {
        "max_iter": 500,
        "sample_style": "constant",
        "reduce_variance": true,
        "measure_freq": 5
      }
    },
    "svrg": {
      "solver": "svrg",
      "alpha_min": -2,
      "alpha_max": 1,
      "n_alpha": 20,
      "batch": [0.001,0.005,0.01],
      "params": {
        "n_epochs": 20,
        "measure_freq": 10
      }
    },
    "saga": {
      "solver": "saga",
      "alpha_min": -3,
      "alpha_max": -1,
      "n_alpha": 20,
      "batch": [],
      "params": {
        "n_epochs": 20,
        "measure_freq": 10
      }
    }
  },
  "psi_tol": 0.0001,
  "n_rep": 5,
  "start": 1
}
